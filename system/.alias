alias g="git"
alias v="vim"
alias vd="vim ."
alias st="st -nc"
alias srv="st"

alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias ~="cd ~"
alias l="ls -al"
alias t1="tree -L 1"
alias t2="tree -L 2"
alias t3="tree -L 3"
alias t4="tree -L 4"
alias t5="tree -L 5"
alias t6="tree -L 6"
alias c="clear"

alias reload="source ~/.zshrc"
alias vimrc="vim ~/.vimrc"
alias vundlerc="vim ~/.vim/vundle.vim"
alias zshrc="vim ~/.zshrc && reload"
alias dotfiles="cd ~/repos/dotfiles && v ."
alias gitconf="vim ~/.gitconfig"

alias pipit="pip install -r requirements.txt"

alias swank="sbcl --load ~/.vim/slime/start-swank.lisp"

alias rb="ruby"
alias py="python"

alias dockip="boot2docker ip"
alias d="docker"
