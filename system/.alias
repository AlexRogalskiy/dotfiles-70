alias g="git"
alias v="vim"
alias srv="python -m SimpleHTTPServer"

alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias ~="cd ~"

alias reload="source ~/.zshrc"
alias vimrc="vim ~/.vimrc"
